	.data
v$a:	.space	4
v$b:	.space	4

	.text
__start:
	jal	main
	li	$v0, 10
	syscall          # stoppe l'execution du processus
pgcd:
	subi	$sp, $sp, 4      # debut fonction
	sw	$fp, 0($sp)      # debut fonction
	move	$fp, $sp         # debut fonction
	subi	$sp, $sp, 4      # debut fonction
	sw	$ra, 0($sp)      # debut fonction
	subi	$sp, $sp, 4      # allocation variables locales
	lw	$t1, 8($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, 8($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, 4($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	div	$t0, $t1
	mflo	$t2
	subi	$sp, $sp, 4      # Empile
	sw	$t2, 0($sp)      # Empile
	lw	$t1, 4($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	mult	$t0, $t1
	mflo	$t2
	subi	$sp, $sp, 4      # Empile
	sw	$t2, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sub	$t2, $t0, $t1
	subi	$sp, $sp, 4      # Empile
	sw	$t2, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sw	$t1, -8($fp)
	lw	$t1, -8($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	li	$t0, 0
	subi	$sp, $sp, 4      # Empile
	sw	$t0, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	li	$t2, -1
	beq	$t0, $t1, e2
	li	$t2, 0
e2:
	subi	$sp, $sp, 4      # Empile
	sw	$t2, 0($sp)      # Empile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	beq	$t0, $zero, e0    # IF
	lw	$t1, 4($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sw	$t0, 12($fp)
	addi	$sp, $sp, 4      # desallocation variables locales
	lw	$ra, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	lw	$fp, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	jr	$ra              # fin fonction
	j	e1
e0:
	subi	$sp, $sp, 4     # allocation valeur de retour
	lw	$t1, 4($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, -8($fp)
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	jal	pgcd
	addi	$sp, $sp, 8     # desallocation parametres
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sw	$t0, 12($fp)
	addi	$sp, $sp, 4      # desallocation variables locales
	lw	$ra, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	lw	$fp, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	jr	$ra              # fin fonction
e1:
	addi	$sp, $sp, 4      # desallocation variables locales
	lw	$ra, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	lw	$fp, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	jr	$ra              # fin fonction
main:
	subi	$sp, $sp, 4      # debut fonction
	sw	$fp, 0($sp)      # debut fonction
	move	$fp, $sp         # debut fonction
	subi	$sp, $sp, 4      # debut fonction
	sw	$ra, 0($sp)      # debut fonction
	li	$v0, 5
	syscall
	subi	$sp, $sp, 4      # Empile
	sw	$v0, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sw	$t1, v$a
	li	$v0, 5
	syscall
	subi	$sp, $sp, 4      # Empile
	sw	$v0, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	sw	$t1, v$b
	lw	$t1, v$b
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, v$a
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	li	$t2, -1
	blt	$t0, $t1, e5
	li	$t2, 0
e5:
	subi	$sp, $sp, 4      # Empile
	sw	$t2, 0($sp)      # Empile
	lw	$t0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	beq	$t0, $zero, e3    # IF
	subi	$sp, $sp, 4     # allocation valeur de retour
	lw	$t1, v$a
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, v$b
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	jal	pgcd
	addi	$sp, $sp, 8     # desallocation parametres
	lw	$a0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	li	$v0, 1
	syscall
	li	$a0, '\n'
	li	$v0, 11
	syscall
	j	e4
e3:
	subi	$sp, $sp, 4     # allocation valeur de retour
	lw	$t1, v$b
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	lw	$t1, v$a
	subi	$sp, $sp, 4      # Empile
	sw	$t1, 0($sp)      # Empile
	jal	pgcd
	addi	$sp, $sp, 8     # desallocation parametres
	lw	$a0, 0($sp)      # Depile
	addi	$sp, $sp, 4      # Depile
	li	$v0, 1
	syscall
	li	$a0, '\n'
	li	$v0, 11
	syscall
e4:
	lw	$ra, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	lw	$fp, 0($sp)      # fin fonction
	addi	$sp, $sp, 4      # fin fonction
	jr	$ra              # fin fonction
